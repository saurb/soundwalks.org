- form_for(:study, :url => sendstudy_path) do |form|
	- @sounds.each do |sound|
		%p
			%object{:type=>"audio/x-wav", :data=>"#{soundwalk_sound_path(sound.soundwalk, sound)}.wav", :width=>"200", :height=>"20"}
				%param{:name=>"src", :value=>"#{soundwalk_sound_path(sound.soundwalk, sound)}.wav"}
				%param{:name=>"autoplay", :value=>"false"}
				%param{:name=>"autoStart", :value=>"0"}
				alt : 
				%a{:href=>"#{soundwalk_sound_path(sound.soundwalk, sound)}.wav"} "#{soundwalk_sound_path(sound.soundwalk, sound)}.wav"
			%input{:type => 'text', :name => "tags[]"}
			%input{:type => 'hidden', :name => "ids[]", :value => sound.id}
	%p
		= form.submit "Send"
