<h1>All users</h1>

<p><%= link_to 'New user', new_user_path %></p>

<table>
	<tr>
		<th>Name</th>
		<th>Soundwalks</th>
		<th>Created at</th>
		<th>Updated at</th>
	</tr>
<% @users.each do |user| %>
	<tr>
		<td><%= link_to user.name, user %></td>
		<td><%= user.soundwalks.size %></td>
		<td><%= user.created_at %></td>
		<td><%= user.updated_at %></td>
	</tr>
<% end %>
</table>