<table>
	<tr>
		<th>Title</th>
		<th>Description</th>
		<th>Created at</th>
		<th>Updated at</th>
	</tr>
<% @soundwalks.each do |soundwalk| %>
	<tr>
		<td><%= link_to soundwalk.title, user_soundwalk_path(@user, soundwalk) %></td>
		<td><%= soundwalk.description %></td>
		<td><%= soundwalk.created_at %></td>
		<td><%= soundwalk.updated_at %></td>
	</tr>
<% end %>
</table>